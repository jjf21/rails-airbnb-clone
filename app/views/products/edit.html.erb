<%= simple_form_for @product do |f|%>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :description %>
  <%= f.input :date_of_birth %>
  <%= f.input :price %>
  <%= f.input :phone_number %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :height %>
  <%= f.input :weight %>
  <%= f.input :shoes %>
  <%= f.input :eyes %>
  <%= f.input :hair %>
  <%= f.submit %>
<% end %> 